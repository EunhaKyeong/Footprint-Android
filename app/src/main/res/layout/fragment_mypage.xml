<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/test_google_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="구글 로그아웃"
        style="@style/tv_subtitle_b_16"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/test_google_unlink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="구글 탈퇴"
        style="@style/tv_subtitle_b_16"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/test_google_logout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/test_kakao_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="카카오 로그아웃"
        style="@style/tv_subtitle_b_16"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/test_google_unlink"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/test_kakao_unlink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="카카오 탈퇴"
        style="@style/tv_subtitle_b_16"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/test_kakao_logout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>